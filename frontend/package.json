{
  "name": "smart-brain-frontend",
  "version": "0.1.0",
  "description": "AI image detection web application frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "devmode": "pnpm run build-backend && pnpm run start-backend && pnpm run dev",
    "build-and-start-frontend": "pnpm run build && pnpm run start",
    "start-backend": "pnpm --prefix ../backend run start",
    "build-backend": "pnpm --prefix ../backend run build",
    "install-backend": "pnpm --prefix ../backend install",
    "install-frontend": "pnpm install",
    "install-both": "pnpm run install-backend && pnpm run install-frontend",
    "build-and-start": "cross-env NODE_ENV=production pnpm run install-backend && pnpm run build && pnpm run start && build-backend & pnpm run start-backend",
    "cypress:open": "cypress open",
    "cypress:test": "concurrently \"pnpm run start-full \" \"pnpm run cypress:open \""
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@graphql-tools/utils": "^10.0.13",
    "@mui/base": "5.0.0-beta.38",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/x-data-grid": "^6.19.1",
    "@tsparticles/engine": "^3.1.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.1.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "framer-motion": "^10.18.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.29",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-groups": "^2.2.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
}
